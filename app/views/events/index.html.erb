<% content_for :title, "Events" %>
<div id="content-wrapper">
<div id="single-column-wrapper">
<div id="content">
<p id="notice"><%= notice %></p>

<h1>College STAR Events</h1>
<%= render( 
      'shared/bread_crumb_social_media', 
      bread_crumb_args: {
        page_name: 'events'
      }
    )
%>

<p>Below you will find a list of conferences that are typically for higher education professionals implementing UDL.</p>

<%  @events.each do |event| %>
  <div class="event">
    <h3><%= link_to event.name.html_safe, event_path( event )  %> - <%= event.start_date.strftime("%B %d, %Y") %></h3>
    <%= event.description.html_safe %>
  </div>
<% end %>
</div>
</div>
</div>
